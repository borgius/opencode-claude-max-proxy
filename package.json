{
  "name": "opencode-claude-proxy",
  "version": "4.0.0",
  "description": "Cloudflare Containers proxy for OpenCode using Claude Max subscription with full OpenAI API compatibility",
  "type": "module",
  "main": "dist/container-server.js",
  "scripts": {
    "build": "vite build --config vite.build.config.ts",
    "build:watch": "vite build --config vite.build.config.ts --watch",
    "start": "node dist/container-server.js",
    "start:dev": "node --import tsx src/container-server.ts",
    "dev": "wrangler dev",
    "deploy": "./deploy.sh",
    "deploy:manual": "wrangler deploy",
    "update-creds": "./update-creds.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "vitest run --config vitest.integration.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "vitest run && vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:local": "./test-local.sh",
    "typecheck": "tsc --noEmit",
    "tail": "wrangler tail",
    "containers:list": "wrangler containers list",
    "containers:images": "wrangler containers images list"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.1",
    "@cloudflare/containers": "^0.1.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.27",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2",
    "vite": "^6.0.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.61.1"
  },
  "keywords": [
    "opencode",
    "claude",
    "anthropic",
    "cloudflare-workers",
    "cloudflare-containers",
    "claude-max",
    "proxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rynfar/opencode-claude-max-proxy.git"
  },
  "homepage": "https://github.com/rynfar/opencode-claude-max-proxy#readme",
  "bugs": {
    "url": "https://github.com/rynfar/opencode-claude-max-proxy/issues"
  },
  "author": "rynfar",
  "license": "MIT"
}
